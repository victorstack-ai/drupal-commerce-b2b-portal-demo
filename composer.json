{
  "name": "victorstack-ai/drupal-commerce-b2b-portal-demo",
  "description": "Demo Drupal module for a B2B portal pattern on Drupal Commerce sites.",
  "type": "drupal-module",
  "license": "MIT",
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "squizlabs/php_codesniffer": "^3.10"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\commerce_b2b_portal_demo\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Drupal\\commerce_b2b_portal_demo\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "lint": "vendor/bin/phpcs",
    "test": "vendor/bin/phpunit"
  }
}
